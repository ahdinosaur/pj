{
  "name": "pj",
  "version": "0.0.0",
  "description": "pixel jockey",
  "main": "index.js",
  "scripts": {
    "start:dev": "npm-run-all -p start types:watch",
    "start": "electron .",
    "types:watch": "tsc -w --noEmit",
    "types:check": "tsc --noEmit",
    "test:deps": "dependency-check . leader follower && dependency-check . leader follower --extra --no-dev",
    "test:lint": "standard",
    "test:node": "NODE_ENV=test run-default tape test/*.js --",
    "test:coverage": "NODE_ENV=test nyc npm run test:node",
    "test:coverage:report": "nyc report --reporter=lcov npm run test:node",
    "test": "npm-run-all -s test:node test:lint test:deps"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahdinosaur/pj.git"
  },
  "keywords": [],
  "author": "Mikey <michael.williams@enspiral.com> (http://dinosaur.is)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/ahdinosaur/pj/issues"
  },
  "homepage": "https://github.com/ahdinosaur/pj#readme",
  "devDependencies": {
    "@types/node": "^7.0.29",
    "@types/ramda": "0.0.11",
    "dependency-check": "^2.7.0",
    "electron-prebuilt-compile": "^1.6.11",
    "npm-run-all": "^4.0.1",
    "nyc": "^10.1.2",
    "run-default": "^1.0.0",
    "standard": "^8.6.0",
    "tape": "^4.6.3",
    "typescript": "^2.3.4"
  },
  "dependencies": {
    "@cycle/dom": "^17.4.0",
    "@cycle/isolate": "^2.1.0",
    "@cycle/run": "^3.1.0",
    "bonjour": "^3.5.0",
    "cuid": "^1.3.8",
    "cycle-onionify": "^3.3.0",
    "cycle-regl": "0.0.1",
    "getport": "^0.1.0",
    "insert-css": "^2.0.0",
    "ndarray": "^1.0.18",
    "opc": "ahdinosaur/js-opc#codec",
    "pixels-gl": "^2.0.0",
    "pixels-opc": "^4.0.2",
    "pull-opc": "^1.0.1",
    "pull-stream": "^3.5.0",
    "ramda": "^0.23.0",
    "regl": "^1.3.0",
    "stream-to-pull-stream": "^1.7.2",
    "xstream": "^10.8.0",
    "xstream-connect": "^0.2.3"
  }
